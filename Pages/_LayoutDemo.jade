block var
doctype html
html.no-js(lang='en')
  head
    meta(charset='utf-8')
    title #{name}, Style Guide | Scriptura Framework
    meta(name='description' content='#{name} for Scriptura Framework')
    include ../Includes/Metas
  body(itemscope itemtype='https://schema.org/WebPage')
    include ../Includes/MainNav
    main(itemprop='mainContentOfPage').section
      .wrap
        h1(itemprop='headline name').emphasized #{name}
        block content
          p A content...
    footer.section.footer
      include ../Includes/Footer
    include ../Includes/Scripts