doctype html
html.no-js(lang='en')
  head
    meta(charset='utf-8')
    title Grid Layout, Style Guide | Scriptura Framework
    meta(name='description' content='Grid Layout for Scriptura Framework')
    include ../Includes/Metas.jade
  body(itemscope itemtype='https://schema.org/WebPage')
    include ../Includes/MainNav.jade
    main(itemprop='mainContentOfPage').section
      .wrap
        h1(itemprop='headline name').emphasized Grid Layout
        include ../Includes/GridLayout.jade
    footer.section.footer
      include ../Includes/Footer.jade
    include ../Includes/Scripts.jade