mixin list(icon, name, slug)
  li
    svg.icon-inline(role='img' focusable='false')
      use(href='/sprites/util.svg#' + icon)
    //- slug non utilis√©.
    a(href='#') #{name}

.grid4.m-grid2.gap.gap-bottom
  div
    p
      code ul
    ul
      li Artichauts
      li Carottes
      li Concombres
      li Poireaux
      li Choux
        ul
          li Composition
          li Saveur
          li Taille
          li Couleurs
            ul
              li Cyan
              li Magenta
              li Jaune
              li Noir
  div
    p
      code ol
    ol
      li Artichauts
      li Carottes
      li Concombres
      li Poireaux
      li Choux
        ol
          li Composition
          li Saveur
          li Taille
          li Couleurs
            ol
              li Cyan
              li Magenta
              li Jaune
              li Noir
  figure.center.c2
    svg.size.decorative-svg.xs-hidden.s-hidden.m-hidden(role='img' focusable='false')
      use(href='/sprites/silos/245v-d1.svg#1')
  div
    p
      code Basic list
    p A paragraph...
    ul
      li Artichauts
      li Carottes
      li Concombres
      li Poireaux
      li Choux
        ul
          li Composition
          li Saveur
          li Taille
          li Couleurs
            ul
              li Cyan
              li Magenta
              li Jaune
              li Noir
  div
    p
      code ul(style='--list-start:1.3em')
    p A paragraph...
    ul(style='--list-start:1.3em')
      li Artichauts
      li Carottes
      li Concombres
      li Poireaux
      li Choux
        ul
          li Composition
          li Saveur
          li Taille
          li Couleurs
            ul
              li Cyan
              li Magenta
              li Jaune
              li Noir
  div
    p
      code ol.list-gap
    ol.list-gap(style='--list-start:1.3em')
      - for (let i = 1; i < 11; i++)
        li List item #{i}
  div
    p
      code ol.list-gap(start='0')
    ol.list-gap(start='0' style='--list-start:1.3em')
      li.chorus
        code li.chorus
        |  List item 1
      - for (let i = 2; i < 11; i++)
        li List item #{i}
.grid-auto.gap.gap-bottom
  div
    p
      code ul.list-stripe
    ul.grid.list-stripe
      - for (let i = 1; i < 8; i++)
        li List item #{i}
  div
    p
      code ul.list-stripe
    ul.grid.list-stripe
      - for (let i = 1; i < 8; i++)
        li
          svg.icon-inline(role='img' focusable='false')
            use(href='/sprites/util.svg#quill')
          | List item #{i}
  div
    p
      code ul.list-stripe li a
    ul.grid.list-stripe
      - for (let i = 1; i < 8; i++)
        li
          a(href='#') List item #{i}
  div
    p
      code ul.list-stripe-links
    ul.grid.list-stripe-links
      - for (let i = 1; i < 8; i++)
        li
          a(href='#')
            svg.icon-inline(role='img' focusable='false')
              use(href='/sprites/util.svg#link')
            | List item #{i}
